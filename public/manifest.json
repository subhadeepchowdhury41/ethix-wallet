{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Ethix Wallet",
  "description": "Chrome extension to connect you to Ethereum blockchain",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://*/",
    "http://*/"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Ethix Wallet",
    "default_icon": "logo.png"
  }
}